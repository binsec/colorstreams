# use inside nix shell
args=-MDBI -e 'DBI::_new_drh(q{}, {}, 0)'
main=main
tag=sinkgen
tag=sourcegen
p=compose:sourcegen
sourcegen:compose-p=autosource
autosource-source-stubs=main:cmd_arg(3,cmdarg,controlled)
sourcegen:compose-c=compose:sinkgen
sinkgen:compose-p=cfhautosink
cfhautosink-byte-split
sinkgen:compose-c=symbolic
symbolic-src-bytes=cmdarg <0>:688-695
symbolic-p=WeakControl;NotStrongControl;Count;S&S
sns-l=10
# concretely execute memory allocation + output + hash
symbolic-cw=malloc@plt;calloc@plt;free@plt;_IO_printf;_IO_puts;S_perl_hash_siphash_1_3_with_state_64.constprop.0
symbolic-crwa
